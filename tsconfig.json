{
	"extends": "@total-typescript/tsconfig/tsc/dom/library",
	"include": ["src/**/*.ts"],
	"exclude": ["src/**/*.test.ts"],
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"declarationDir": "./build",
		"moduleResolution": "bundler",
		"module": "ESNext",
		"outDir": "./build",
		"target": "ESNext"
	}
}
